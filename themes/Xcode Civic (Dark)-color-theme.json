	"tokenColors": [
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "foreground": "#51C34F"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["string constant.other.placeholder"],
		"settings": {
		  "foreground": "#e7e8eb"
		}
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": {
		  "foreground": "#53B3B1"
		}
	  },
	  {
		"name": "Numbers",
		"scope": ["constant.numeric"],
		"settings": {
		  "foreground": "#34c9c6",
		  "fontStyle": ""
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#FF5370"
		}
	  },
	  {
		"name": "Keyword, Storage",
		"scope": ["storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#E12DA0"
		}
	  },
	  {
		"name": "Keyword, Storage",
		"scope": ["keyword"],
		"settings": {
		  "foreground": "#E7E8EB"
		}
	  },
	  {
		"name": "Operator, Misc",
		"scope": [
		  "constant.other.color",
		  "meta.tag",
		  "meta.brace.round",
		  "meta.parameters",
		  "punctuation.definition.tag",
		  "punctuation.separator.inheritance.php",
		  "punctuation.section.embedded",
		  "keyword.other.template",
		  "keyword.other.substitution"
		],
		"settings": {
		  "foreground": "#E7E8EB"
		}
	  },
	  {
		"name": "Tag",
		"scope": [
		  "entity.name.tag",
		  "meta.tag.sgml",
		  "markup.deleted.git_gutter"
		],
		"settings": {
		  "foreground": "#E12DA0"
		}
	  },
	  {
		"name": "Function, Special Method",
		"scope": [
		  "meta.function-call",
		  "variable.function",
		  "support.function",
		  "keyword.other.special-method"
		],
		"settings": {
		  "foreground": "#53B3B1"
		}
	  },
